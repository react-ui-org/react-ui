{
  "extends" : [
    "@visionappscz/eslint-config-visionapps"
  ],
  "env": {
    "browser": true,
    "jest": true
  },
  "overrides": [
    {
      "files": ["**/*.md", "**/*.mdx"],
      "extends": [
        "plugin:markdown/recommended",
        "plugin:mdx/recommended"
      ]
    }
  ],
  "parser": "@babel/eslint-parser",
  "rules": {
    "import/prefer-default-export": "off"
  }
}
